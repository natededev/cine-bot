# CineBot Frontend Environment Variables - Example File
# Copy this file to .env and customize as needed

# API Configuration
# Development: http://localhost:8000 (local backend)
# Production: Set in Vercel to your EC2 backend URL (e.g., http://your-ec2-ip:8000)
VITE_API_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=false

# UI Configuration
VITE_TYPING_INDICATOR_DELAY=5000
VITE_MAX_MESSAGES=100

# Performance Settings
VITE_ENABLE_PERFORMANCE_MONITORING=true
